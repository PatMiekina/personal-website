<div class="container container-form project-form">
  <%= simple_form_for @project do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :path %>
    <%= f.input :website %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Add a project", class: "btn btn-main" %>
  <% end %>
</div>
